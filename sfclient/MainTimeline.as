                        };
                        if ((((textWidth <= SIZE_GILDE_TEXT_X)) && ((line.substr((line.length - 1)) == " ")))){
                            seekSpace = false;
                        } else {
                            nextLine = (line.substr((line.length - 1)) + nextLine);
                        };
                        line = line.substr(0, (line.length - 1));
                        if (line == ""){
                            line = nextLine;
                            nextLine = "";
                            noSpace = true;
                            seekSpace = false;
